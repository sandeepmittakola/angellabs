<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>


	<class name="co.angellabs.entities.dto.Company" table="company">
		<id name="companyId" column="companyid">
			<generator class="increment">
			</generator>
		</id>
		<property name="companyName">
			<column name="companyname" />
		</property>
		<property name="companyLogo">
			<column name="logo" />
		</property>
		<property name="website">
			<column name="website" />
		</property>
		<property name="highConnectPitch">
			<column name="highconnectpitch" />
		</property>
		<property name="companytypeId">
			<column name="companytypeid" />
		</property>
		<property name="companysizeId">
			<column name="companysizeid" />
		</property>
		<property name="companylocationId">
			<column name="companylocationId" />
		</property>
		<property name="createdDate">
			<column name="createddate" />
		</property>
	</class>
</hibernate-mapping>