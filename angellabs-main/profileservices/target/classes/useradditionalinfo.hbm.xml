<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>


	<class name="co.angellabs.entities.dto.UserAdditionalInfo" table="useradditionalinfo">
		<id name="useradditionalInfoId" column="useradditionalinfoid">
			<generator class="increment">
			</generator>
		</id>
		<property name="miniresume">
			<column name="miniresume" />
		</property>
		<property name="summary">
			<column name="summary" />
		</property>
		<property name="achievment">
			<column name="achievment" />
		</property>
		<property name="lookingfor">
			<column name="lookingFor" />
		</property>
		<property name="photo">
			<column name="photo" />
		</property>
		<property name="userId">
			<column name="userid" />
		</property>
		
	</class>
</hibernate-mapping>